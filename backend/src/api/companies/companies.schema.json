{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Company",
  "description": "The Company",
  "required": [
      "name",
      "address_id"
  ],
  "additionalProperties": false,
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The database ID of the company."
      },
      "name": {
          "$id": "#/properties/name",
          "type": "string",
          "title": "The name of the company."
      },
      "logo_url": {
          "$id": "#/properties/logo_url",
          "type": "string",
          "title": "The company logo url."
      },
      "description": {
          "$id": "#/properties/description",
          "type": "string",
          "title": "The company description"
      },
      "website_url": {
          "$id": "#/properties/website_url",
          "type": "string",
          "title": "The website url."
      },
      "email": {
          "$id": "#/properties/email",
          "type": "string",
          "title": "The company email."
      },
      "address_id": {
          "$id": "#/properties/address_id",
          "type": "number",
          "title": "The id of the address."
      }
  }
}